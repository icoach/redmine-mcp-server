{
  "name": "@icoach/redmine-mcp-server",
  "version": "1.0.0",
  "description": "A Model Context Protocol (MCP) server for Redmine API integration",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "redmine-mcp-server": "dist/index.js"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "npm run build && node test-integration.js",
    "test:full": "npm run build && node tests/api-endpoints.test.js",
    "build": "tsc",
    "prepack": "npm run build",
    "start": "node dist/index.js",
    "dev": "ts-node --esm src/index.ts",
    "watch": "nodemon --exec \"npm run dev\""
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "redmine",
    "api",
    "integration"
  ],
  "author": "icoach",
  "license": "ISC",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.3",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.13.10",
    "axios": "^1.8.3",
    "eventsource": "^3.0.5",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
